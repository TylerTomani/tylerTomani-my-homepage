    "def findPrimes(num):\n",
    "    primes = [2]\n",
    "    for n in range(3,num):\n",
    "        sqrtN = n ** 0.5\n",
    "        for factor in primes:\n",
    "            if n % factor == 0:\n",
    "                break\n",
    "            if factor > sqrtN:\n",
    "                primes.append(n)\n",
    "    return primes\n",
    "print(findPrimes(22))"
   ]
  }
